    "express": "^4.19.2",
    "express-validator": "^5.3.0"
  },
  "devDependencies": {
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/mocha": "^5.2.5",
    "@types/node": "^12.12.17",
    "cross-env": "^7.0.3",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "mocha": "10.1.0",
    "nodemon": "^3.1.1",
    "prettier": "^3.0.0",
    "typescript": "5.0.3",
    "typescript-eslint-parser": "^18.0.0",
    "tslint": "^5.11.0",
    "ts-node": "^10.9.1"
